{
    "name": "zsanmartin/laravel-slashid",
    "description": "SlashID integration package for Laravel.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SlashId\\Laravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jos√© San Martin",
            "email": "jose@josesanmartin.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "zsanmartin/slashid-php": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SlashId\\Laravel\\Providers\\SlashIdServiceProvider"
            ],
            "aliases": {
                "slashid_group": "SlashId\\Laravel\\Middleware\\GroupAccess"
            }
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/zsanmartin/slashid-php"
        }
    ],
    "require-dev": {
        "laravel/pint": "^1.14",
        "phpunit/phpunit": "^10.5"
    },
    "scripts": {
        "test-pint": "./vendor/bin/pint --test",
        "test-pint-fix": "./vendor/bin/pint --test"
    }
}
